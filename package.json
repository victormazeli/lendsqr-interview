{
  "name": "lendsqr-test",
  "version": "1.0.0",
  "description": "lendsqr test interview",
  "main": "server.js",
  "scripts": {
    "start": "npm run prod",
    "transpile": "babel ./src --out-dir dist-server",
    "clean": "rimraf dist-server",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist-server/swagger",
    "dev": "cross-env NODE_ENV=development npm-run-all build server",
    "prod": "cross-env NODE_ENV=production npm-run-all build server",
    "watch:dev": "nodemon",
    "test": "nyc --reporter=html mocha --require @babel/register src/test/specs/**/*spec.js --exit && nyc report --timeout 10000",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint . --fix",
    "db:migrate": "knex migrate:latest --knexfile ./knexfile.js",
    "db:migrate:down": "knex migrate:down --knexfile ./knexfile.js",
    "db:seed": "knex seed:run"
  },
  "author": "victor chidubem mazeli",
  "license": "ISC",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "amqplib": "^0.8.0",
    "app-root-path": "^3.0.0",
    "awilix-express": "^6.0.0",
    "axios": "^0.26.0",
    "bcrypt": "^5.0.1",
    "bull": "^4.2.1",
    "celebrate": "^15.0.1",
    "cloudinary": "^1.27.1",
    "config": "^3.3.7",
    "coveralls": "^3.1.1",
    "cron": "^1.8.2",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-joi-validation": "^5.0.1",
    "express-validator": "^6.14.0",
    "google-auth-library": "^7.12.0",
    "googleapis": "^92.0.0",
    "http-status": "^1.5.0",
    "ioredis": "^4.28.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.3",
    "moment": "^2.29.1",
    "mongodb-memory-server": "^8.0.2",
    "mongodb-memory-server-core": "^8.2.0",
    "mongoose": "^6.0.12",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.2",
    "npm-run-all": "^4.1.5",
    "onesignal-node": "^3.3.0",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "randomstring": "^1.2.2",
    "redis": "^4.0.1",
    "rimraf": "^3.0.2",
    "short-unique-id": "^4.4.4",
    "spdy": "^4.0.2",
    "supertest": "^6.2.2",
    "swagger-autogen": "^2.18.8",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "twilio": "^3.74.0",
    "uuidv4": "^6.2.12",
    "vhost": "^3.0.2",
    "winston": "^3.3.3",
    "winston-loki": "^6.0.3",
    "winston3-logstash-transport": "^1.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/register": "^7.17.0",
    "@faker-js/faker": "^6.0.0-beta.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "jest": "^27.3.1",
    "mocha": "^9.2.1",
    "nodemon": "^2.0.14",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1"
  },
  "engines": {
    "node": "16.14.0"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  }
}
